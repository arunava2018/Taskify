# Frontend URL
# The URL where your frontend is deployed or runs locally
FRONTEND_URL=http://localhost:5173

# MongoDB Atlas Connection
# Replace <username>, <password>, and <cluster-url> with your own
# and make sure to include your actual database name (e.g., Taskify)
MONGO_URI=mongodb+srv://<username>:<password>@<cluster-url>.mongodb.net/Taskify?retryWrites=true&w=majority

# Clerk Authentication Keys
# You can get these from your Clerk Dashboard â†’ API Keys section
CLERK_PUBLISHABLE_KEY=pk_test_your_clerk_publishable_key_here
CLERK_SECRET_KEY=sk_test_your_clerk_secret_key_here

# Clerk Webhook Secret (for verifying incoming webhooks)
CLERK_WEBHOOK_SIGNING_SECRET=whsec_your_clerk_webhook_secret_here
